.user-summary-view {
    font-size:28rpx;
    padding: 30rpx 20rpx;
    border-bottom: 1rpx solid #D9D9D9;
}

.age-constellation {
    margin-top: 5rpx;
    margin-bottom: 10rpx;
}

.age-male {
    float:left;
    font-size: 20rpx;
    margin-right:10rpx;
    padding: 3rpx 20rpx;
    border-radius: 10rpx;
    background-color: #7ae4ff;
}

.constellation {
    font-size: 20rpx;
    padding: 3rpx 20rpx;
    border-radius: 10rpx;
    background-color: #ffd6ab;
    width:60rpx;
    margin-left:70rpx;
}

.tips {
    font-size: 24rpx;
    color: #737373;
}

.icon-right {
    width: 10rpx;
    height: 16rpx;
    position: absolute;
}
