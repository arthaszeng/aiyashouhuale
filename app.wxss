/**app.wxss**/
@import "weui.wxss";

page{
  height:100%;
}
.container {
  height: 100%;
  width:100%;
  display: flex;
  flex-direction: column;
/*align-items: center;*/
box-sizing: border-box;
}

.topbar-view {
  height: 60rpx;
  width: 100%;
  background-color: #FED440;
}
.title-view {
  font-size:30rpx;
  position:fixed;
  color:white;
  width:30%;
  height:inherit;
  top:0;
  right:35%;
  text-align:center;
  font-weight:bold;
}

.edit-icon {
  height: 45rpx;
  width: 45rpx;
  top: 2rpx;
  right: 0rpx;
  margin-right: 30rpx;
  position: fixed;
}

.menu-icon {
  height: 45rpx;
  width: 45rpx;
  top: 2rpx;
  margin-left: 30rpx;
}
